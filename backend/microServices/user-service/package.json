{
    "name": "lesson-sevice",
    "version": "1.0.0",
    "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
    "main": "index.js",
    "scripts": {
        "start": "tsc && node ./dist/src/index.js",
        "build": "tsc",
        "startdev": "nodemon -L",
        "test": "cross-env NODE_ENV=test mocha --recursive -r ts-node/register src/**/*.spec.ts --exit",
        "test:validation": "cross-env NODE_ENV=test mocha --recursive -r ts-node/register \"src/**/*.validator.spec.ts\" --exit",
        "test:integration": "cross-env NODE_ENV=test mocha --recursive -r ts-node/register \"src/**/*.router.spec.ts\" --exit",
        "test:repository": "cross-env NODE_ENV=test mocha --recursive -r ts-node/register \"src/**/*.repository.spec.ts\" --exit",
        "test:coverage": "cross-env NODE_ENV=test nyc npm test",
        "coverage-test": "cross-env NODE_ENV=test nyc check-coverage --lines 95 --functions 95 --branches 95",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "release": "standard-version",
        "pretty": "prettier --list-different \"**/*.{ts,json}\" -w",
        "lint:fix": "eslint \"**/*.{ts,js}\" --fix",
        "nodemon": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/index.ts\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.com//lesson-sevice/lesson-sevice.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://gitlab.com//lesson-sevice/lesson-sevice/issues"
    },
    "homepage": "https://gitlab.com//lesson-sevice/lesson-sevice#readme",
    "dependencies": {
        "@azure/identity": "^3.0.0",
        "@azure/storage-blob": "^12.12.0",
        "@joi/date": "^2.1.0",
        "@turf/turf": "^6.5.0",
        "@types/joi": "^17.2.3",
        "@types/jsonwebtoken": "^8.5.9",
        "cls-hooked": "^4.2.2",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "express": "^4.16.4",
        "common-atom": "^1.0.1",
        "shared-atom": "^1.0.2",
        "formidable": "^2.1.1",
        "jayson": "^4.0.0",
        "joi": "^17.6.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.3.8",
        "mongoose-fuzzy-searching": "^2.0.2",
        "morgan": "^1.10.0",
        "passport-azure-ad": "^4.3.4",
        "socket.io": "^4.5.3",
        "uuid": "^9.0.0",
        "winston": "^3.8.1",
        "ws": "^8.8.1"
    },
    "devDependencies": {
        "@types/cls-hooked": "^4.3.3",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/formidable": "^2.0.5",
        "@types/mongoose": "^5.7.36",
        "@types/morgan": "^1.9.3",
        "@types/passport-azure-ad": "^4.3.1",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "eslint": "^8.23.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-prettier": "^4.0.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5"
    }
}
